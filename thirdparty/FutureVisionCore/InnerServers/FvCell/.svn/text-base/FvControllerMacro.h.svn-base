#define FV_CONTROLLER_NEW( POINTER, CONSTRUCTOR )					\
	POINTER = new CONSTRUCTOR;										\
	*(bool*)(&POINTER->m_bIsNew) = true;

#define FV_CONTROLLER_DELETE( POINTER )								\
	POINTER->Destroy();												\
	*(bool*)(&POINTER->m_bIsNew) = true;

#define FV_DECLARE_CONTROLLER( CLASS )								\
public:																\
	CLASS(){}														\
	void Serializer(FvBinaryOStream& kOStream);						\
	void Unserializer(FvBinaryIStream& kIStream);					\
	FV_INLINE const FvUInt16 GetType()								\
	{																\
		return ms_kFactory.GetType();								\
	}																\
	FV_INLINE static FvController* Create()							\
	{																\
		CLASS *pkData = new CLASS;									\
		*(bool*)(&pkData->m_bIsNew) = true;							\
		return pkData;												\
	}																\
	void Excute(FvUInt16 uiMethodID,FvBinaryIStream& kIStream)		\
	{																\
		FV_ASSERT(uiMethodID < ms_kFunctions.size());				\
		ms_kFunctions[uiMethodID]->Excute(this,kIStream);			\
	}																\
	static FvControllerFactory ms_kFactory;							\
	static std::vector<FvControllerFunctionBase<CLASS>*> ms_kFunctions;	\

#define FV_IMPLEMENT_CONTROLLER( CLASS )							\
	FvControllerFactory CLASS::ms_kFactory((CLASS*)NULL,CLASS::Create);\
	std::vector<FvControllerFunctionBase<CLASS>*> CLASS::ms_kFunctions;	\

#define FV_CONTROLLER_FUN_DECLARE( CLASS, FUN, TYPE )				\
	static FvControllerFunction<CLASS,TYPE> FUN##Fun;

#define FV_CONTROLLER_FUN_IMPLEMENT( CLASS, FUN, TYPE )				\
	FvControllerFunction<CLASS,TYPE> CLASS::FUN##Fun(&CLASS::FUN);

#define FV_CONTROLLER_SERIALIZER_HEADER_START( CLASS )				\
	FV_INLINE void CLASS::Serializer(FvBinaryOStream& kOStream){		
#define FV_CONTROLLER_SERIALIZER_PARAM( PARAM )						\
	ControllerSerializer(this,kOStream,PARAM);				
#define FV_CONTROLLER_SERIALIZER_HEADER_END }

#define FV_CONTROLLER_UNSERIALIZER_HEADER_START( CLASS )			\
	FV_INLINE void CLASS::Unserializer(FvBinaryIStream& kIStream){	
#define FV_CONTROLLER_UNSERIALIZER_PARAM( PARAM )					\
	ControllerUnserializer(this,kIStream,PARAM);
#define FV_CONTROLLER_UNSERIALIZER_HEADER_END }

#define FV_CONTROLLER_OBJECT_STREAM( CLASS )						\
template<>															\
FV_INLINE void ControllerSerializer<CLASS>(FvController* pkHost,	\
				FvBinaryOStream& kOStream, CLASS& kValue)			\
{																	\
	FV_ASSERT(pkHost->GetEntity() == kValue.GetEntity());			\
	kValue.Serializer(kOStream);									\
}																	\
template<>															\
FV_INLINE void ControllerSerializer<CLASS*>(FvController* pkHost,	\
				FvBinaryOStream& kOStream, CLASS*& pkValue)			\
{																	\
	if(pkValue != NULL)												\
	{																\
		FV_ASSERT(pkHost->GetEntity() == pkValue->GetEntity());		\
		kOStream << pkValue->GetID();								\
		pkValue->Serializer(kOStream);								\
	}																\
	else															\
	{																\
		kOStream << FvController::INVALID_ID;						\
	}																\
}																	\
template<>															\
FV_INLINE void ControllerUnserializer<CLASS>(FvController* pkHost,	\
				FvBinaryIStream& kIStream, CLASS& kValue)			\
{																	\
	kValue.Unserializer(kIStream);									\
}																	\
template<>															\
FV_INLINE void ControllerUnserializer<CLASS*>(FvController* pkHost,	\
				FvBinaryIStream& kIStream, CLASS*& pkValue)			\
{																	\
	FvUInt16 uiID;													\
	kIStream >> uiID;												\
	if(uiID != FvController::INVALID_ID)							\
	{																\
		pkValue = static_cast<CLASS*>(								\
			pkHost->GetEntity()->GetController(uiID));				\
		pkValue->Unserializer(kIStream);							\
	}																\
	else															\
	{																\
		pkValue = NULL;												\
	}																\
}

